            <Value>=Parameters!Agence.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@facComptaP">
            <Value>=Parameters!TypeFacture.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@typeTiersP">
            <Value>1</Value>
          </QueryParameter>
          <QueryParameter Name="@ProtNoP">
            <Value>=Parameters!PROT_No.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <CommandText>DECLARE @typeTiers AS INT = @typeTiersP
DECLARE @facCompta AS INT = @facComptaP
DECLARE @datedeb AS NVARCHAR(20) = @datedebP
DECLARE @datefin AS NVARCHAR(20) = @datefinP
DECLARE @clientdebut AS NVARCHAR(50) = @clientdebutP
DECLARE @clientfin AS NVARCHAR(50) = @clientfinP
DECLARE @centre AS INT = @centreP
DECLARE @type_reg AS INT = @type_regP
DECLARE @admin INT
DECLARE @ProtNo INT
SET @ProtNo = @ProtNoP
CREATE TABLE #TMPDEPOT (DE_No INT)
SELECT @admin = CASE WHEN PROT_Administrator=1 OR PROT_Right=1 THEN 1 ELSE 0 END FROM F_PROTECTIONCIAL WHERE PROT_No = @ProtNo               
IF (@admin=0)
BEGIN 
	INSERT INTO #TMPDEPOT
    SELECT	A.DE_No 
    FROM	F_DEPOT A
    LEFT JOIN Z_DEPOTUSER D 
		ON A.DE_No=D.DE_No
    WHERE	(1 = (SELECT CASE WHEN PROT_Administrator=1 OR PROT_Right=1 THEN 1 ELSE 0 END FROM F_PROTECTIONCIAL WHERE PROT_No=@ProtNo) OR D.PROT_No =@ProtNo)
    AND IsPrincipal = 1
	AND (@centre = 0 OR A.DE_No = @centre) 
    GROUP BY A.DE_No
END
ELSE 
BEGIN
	INSERT	INTO #TMPDEPOT 
    SELECT DE_No 
    FROM F_DEPOT 
	WHERE (@centre = 0 OR DE_No = @centre) 
END 
;

WITH _Ligne_ AS (
SELECT DO_Domaine,DO_Type,cbDO_Piece,SUM(DL_MontantTTC) DL_MontantTTC
FROM	F_DOCLIGNE L
GROUP BY DO_Domaine,DO_Type,cbDO_Piece
)
,_Entete_ AS (
SELECT DO_Domaine,DO_Type,DO_Piece,cbDO_Piece,DO_Date,DE_No,cbDO_Tiers,DO_Tiers AS CT_Num
FROM F_DOCENTETE
GROUP BY DO_Domaine,DO_Type,DO_Piece,cbDO_Piece,DO_Date,DE_No,cbDO_Tiers,DO_Tiers
)
,_GroupData_ AS (  
SELECT A.DO_Domaine,A.DO_Type,A.DO_Piece,A.cbDO_Piece,A.CT_Num,A.DE_No,cbDO_Tiers,A.DO_Date,DL_MontantTTC
FROM _Entete_ A 
LEFT JOIN _Ligne_ B 
ON  A.DO_Domaine=B.DO_Domaine 
AND A.DO_Type=B.DO_Type 
AND A.cbDO_Piece=B.cbDO_Piece
)
,_ReglEch_ AS (
SELECT 	cbDO_Piece,DO_Domaine
		,DO_Type
		,RC_Montant = SUM(RC_Montant)  
FROM 	F_REGLECH
GROUP BY cbDO_Piece,DO_Domaine,DO_Type
)
,_Requete_ AS (
SELECT 	DE.DE_No,DE.DE_Intitule
		,L.DO_Piece,L.CT_Num, CT_Intitule
		,DO_Date = CAST(L.DO_Date AS DATE)
		,DL_MontantTTC = SUM(DL_MontantTTC) 
		,RC_Montant = ISNULL(SUM(RC_Montant),0)
		,Reste_A_Payer = SUM(DL_MontantTTC) - ISNULL(SUM(RC_Montant),0)
FROM _GroupData_ L
INNER JOIN 	F_COMPTET C 
	ON		L.cbDO_Tiers = C.cbCT_Num 
LEFT JOIN F_DOCREGL D 
	ON 	L.cbDO_Piece=D.cbDO_Piece 
	AND L.DO_Domaine=D.DO_Domaine 
	AND L.DO_Type=D.DO_Type  
LEFT JOIN _ReglEch_ RE 
	ON 	L.cbDO_Piece=RE.cbDO_Piece 
	AND L.DO_Domaine=RE.DO_Domaine 
	AND L.DO_Type=RE.DO_Type
INNER JOIN  F_DEPOT DE 
	ON 	L.DE_No = DE.DE_No 
WHERE	L.DO_Domaine = @typeTiers
AND     ((@facCompta=0 AND (   (@typeTiers =0 AND L.DO_Type in (6,7)) OR (@typeTiers =1 AND L.DO_Type in (16,17)) ) 
			OR (@facCompta=1 AND (@typeTiers =0 AND L.DO_Type in (6)) OR (@typeTiers =1 AND L.DO_Type in (16)) ) 
			OR (@facCompta=2 AND (@typeTiers =0 AND L.DO_Type in (7)) OR (@typeTiers =1 AND L.DO_Type in (17)) ))) 
AND		('' = @datedeb OR l.DO_Date &gt;= @datedeb)
AND     ('-1'= @type_reg OR ISNULL(DR_Regle,0) = @type_reg)
AND     ('' =@datefin OR l.DO_Date&lt;= @datefin)
AND     ('0'=@clientdebut OR L.CT_NUM&gt;=@clientdebut) 
AND 	('0'=@clientfin OR L.CT_NUM&lt;=@clientfin)
AND		L.DE_No IN (SELECT DE_No FROM #TMPDEPOT) 
GROUP BY DE.DE_No,DE.DE_Intitule,L.DO_Piece,L.CT_Num,CT_Intitule, L.DO_Date
)
SELECT 	*
FROM 	_Requete_ A
 WHERE  ('0'&lt;&gt; @type_reg OR (0 = @type_reg AND Reste_A_Payer&lt;&gt;0))
ORDER BY DO_DATE DESC</CommandText>
