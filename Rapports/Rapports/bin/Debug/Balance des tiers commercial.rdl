  <DataSets>
    <DataSet Name="Requete">
      <Query>
        <DataSourceName>DataSource1</DataSourceName>
        <QueryParameters>
          <QueryParameter Name="@CT_NumDebP">
            <Value>=Parameters!ClientDebut.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@CT_NumFinP">
            <Value>=Parameters!ClientFin.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@DO_DateDebP">
            <Value>=Parameters!DateDebut.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@DO_DateFinP">
            <Value>=Parameters!DateFin.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@CT_TypeP">
            <Value>=Parameters!TypeTiers.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@RuptureP">
            <Value>=Parameters!rupture.Value</Value>
            <rd:UserDefined>true</rd:UserDefined>
          </QueryParameter>
          <QueryParameter Name="@agenceP">
            <Value>=Parameters!Agence.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@depotP">
            <Value>=Parameters!Depot.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <CommandText>DECLARE @CT_NumDeb AS VARCHAR(50)=@CT_NumDebP
DECLARE @CT_NumFin AS VARCHAR(50)=@CT_NumFinP
DECLARE @DO_DateDeb AS DATE=@DO_DateDebP
DECLARE @DO_DateFin AS DATE=@DO_DateFinP
DECLARE @CT_Type AS INT=@CT_TypeP
DECLARE @Rupture AS INT=@RuptureP
DECLARE @agence AS INT=@agenceP
DECLARE @depot AS INT=@depotP

SELECT *
FROM(
SELECT  ISNULL(A.CT_Num,B.CT_NumPayeur)CT_Num
		,ISNULL(CT_Intitule,CT_IntituleR)CT_Intitule
		,CASE WHEN @Rupture = 1 THEN A.DE_No ELSE 0 END DE_No
		,CASE WHEN @Rupture = 1 THEN A.DE_Intitule ELSE '' END DE_Intitule
		,ISNULL(SUM(totalFacture),0)totalFacture
		,ISNULL(SUM(TotalReglement),0)TotalReglement
FROM(
SELECT	CT_Num
		,CT_Intitule
		,DE_No
		,DE_Intitule
		,ttc - avance totalFacture
FROM(		
SELECT	E.DO_Tiers as CT_Num
		,CT_Intitule
		,E.DE_No
		,DE_Intitule
		,ROUND(SUM(L.DL_MontantTTC),0) AS ttc
		,ISNULL(sum(avance),0) AS avance
FROM  F_DOCENTETE E 
LEFT JOIN (	SELECT	DO_Piece,DO_Type,DO_Domaine,DO_Ref,DO_Date,CT_Num,SUM(DL_MontantTTC) DL_MontantTTC 
			FROM F_DOCLIGNE L 
			GROUP BY DO_Piece,DO_Type,DO_Domaine,DO_Ref,DO_Date,CT_Num) L on E.DO_Piece=L.DO_Piece  AND E.DO_Domaine= L.DO_Domaine AND E.DO_Type=L.DO_Type
LEFT JOIN F_DOCREGL R on R.DO_Piece=E.DO_Piece AND R.DO_Type=E.DO_Type AND R.DO_Domaine=E.DO_Domaine AND DR_Regle=0
INNER JOIN F_DEPOT D on D.DE_No=E.DE_No 
INNER JOIN F_COMPTET C on C.CT_Num=E.DO_Tiers
LEFT JOIN (	SELECT DR_No
				, SUM(RC_MONTANT) avance 
			FROM F_REGLECH R 
			INNER JOIN F_Creglement Re on Re.RG_No=R.RG_No 
			GROUP BY DR_No) A ON A.DR_No=R.DR_No  
WHERE (@DO_DateDeb='' OR E.DO_Date &gt;= @DO_DateDeb)
AND(@DO_DateFin='' OR E.DO_Date &lt;= @DO_DateFin)
AND (@CT_Type=-1 OR CT_Type=@CT_Type)
GROUP BY E.DO_Tiers,CT_Intitule,E.DE_No,DE_Intitule)A)A
FULL OUTER JOIN (
SELECT CT_NumPayeur,CT_IntituleR,DE_No,SUM(RG_Montant-RC_Montant) TotalReglement
FROM (
SELECT C.CT_NumPayeur
		,CT_Intitule CT_IntituleR 
		,ZD.DE_No
		,SUM(RG_Montant) RG_Montant
		,SUM(ISNULL(RC_Montant,0)) RC_Montant
FROM F_CREGLEMENT C
LEFT JOIN F_COMPTET CT ON C.CT_NumPayeur = CT.CT_Num
LEFT JOIN F_REGLECH R ON C.RG_No = R.RG_No
LEFT JOIN Z_DEPOTCAISSE ZD ON C.CA_No=ZD.CA_No
WHERE	(@DO_DateDeb='' OR RG_Date &gt;= @DO_DateDeb)
AND		(@DO_DateFin='' OR RG_Date &lt;= @DO_DateFin)
AND		(@CT_Type=-1 OR CT_Type=@CT_Type)
GROUP BY C.CT_NumPayeur,CT_Intitule,ZD.DE_No)A
GROUP BY CT_NumPayeur,CT_IntituleR,DE_No
) B ON A.CT_Num=B.CT_NumPayeur AND A.DE_No = B.DE_No
WHERE ((@CT_NumDeb='0' OR CT_Num &gt;= @CT_NumDeb) AND
(@CT_NumFin='0' OR CT_Num &lt;= @CT_NumFin))
GROUP BY ISNULL(A.CT_Num,B.CT_NumPayeur)
		,ISNULL(CT_Intitule,CT_IntituleR)
		,CASE WHEN @Rupture = 1 THEN A.DE_No ELSE 0 END 
		,CASE WHEN @Rupture = 1 THEN A.DE_Intitule ELSE '' END 
		)A
		LEFT JOIN Z_DEPOTSOUCHE ZD ON A.DE_No = ZD.DE_No
		WHERE totalFacture&lt;&gt;0 OR TotalReglement&lt;&gt;0
		AND (@depot = '0' OR @depot = A.DE_No) 
		AND (@agence = '0' OR @agence = CA_Num) 
</CommandText>
      </Query>
