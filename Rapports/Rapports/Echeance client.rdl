          </QueryParameter>
        </QueryParameters>
        <CommandText>DECLARE @type_reg AS VARCHAR(50)
DECLARE @DateDebut AS VARCHAR(50)
DECLARE @DateFin AS VARCHAR(50)
DECLARE @ClientDebut AS VARCHAR(50)
DECLARE @ClientFin AS VARCHAR(50)
DECLARE @Depot AS INT
DECLARE @rupture AS INT
DECLARE @facCompta AS INT
DECLARE @typeTiers AS INT
DECLARE @affaire AS VARCHAR(50)
DECLARE @montant AS FLOAT
DECLARE @signe AS INT

SET @type_reg = @type_regP
SET @DateDebut = @DateDebutP
SET @DateFin = @DateFinP
SET @ClientDebut = @ClientDebutP
SET @ClientFin = @ClientFinP
SET @facCompta = @facComptaP
SET @typeTiers = @typeTiersP
SET @Depot = @DepotP
SET @rupture = @ruptureP
SET @affaire = @affaireP
SET @montant = @montantP
SET @signe = @signeP

CREATE TABLE #TMPDEPOT (DE_No INT)
DECLARE @admin INT
DECLARE @ProtNo INT
SET @ProtNo = @protNoP
SELECT @admin = CASE WHEN PROT_Administrator=1 OR PROT_Right=1 THEN 1 ELSE 0 END FROM F_PROTECTIONCIAL WHERE PROT_No = @ProtNo               
IF (@admin=0)
BEGIN 
	INSERT INTO #TMPDEPOT
    SELECT	A.DE_No 
    FROM	F_DEPOT A
    LEFT JOIN Z_DEPOTUSER D 
		ON A.DE_No=D.DE_No
    WHERE	(1 = (SELECT CASE WHEN PROT_Administrator=1 OR PROT_Right=1 THEN 1 ELSE 0 END FROM F_PROTECTIONCIAL WHERE PROT_No=@ProtNo) OR D.PROT_No =@ProtNo)
    AND IsPrincipal = 1
	AND (@Depot = 0 OR A.DE_No = @Depot) 
    GROUP BY A.DE_No
END
ELSE 
BEGIN
	INSERT	INTO #TMPDEPOT 
    SELECT DE_No 
    FROM F_DEPOT 
	WHERE (@Depot = 0 OR DE_No = @Depot) 
END 
;
WITH _Ligne_ AS (
SELECT DO_Domaine,DO_Type,cbDO_Piece,SUM(DL_MontantTTC) DL_MontantTTC
FROM	F_DOCLIGNE L
GROUP BY DO_Domaine,DO_Type,cbDO_Piece
)
, _Entete_ AS (
SELECT	DO_Domaine,DO_Type,DO_Piece,cbDO_Piece,DO_Date,DE_No,DO_Tiers AS CT_Num,cbDO_Tiers
FROM	F_DOCENTETE
GROUP BY DO_Domaine,DO_Type,DO_Piece,cbDO_Piece,DO_Date,DE_No,DO_Tiers,cbDO_Tiers
)
, _EnteteDoc_ AS (
SELECT	A.DO_Domaine,A.DO_Type,A.DO_Piece,A.cbDO_Piece,A.CT_Num,A.cbDO_Tiers,A.DE_No,A.DO_Date,DL_MontantTTC
FROM	_Entete_ A 
LEFT JOIN _Ligne_ B 
	ON	A.DO_Domaine=B.DO_Domaine 
	AND A.DO_Type=B.DO_Type 
	AND A.cbDO_Piece=B.cbDO_Piece
)
SELECT	*
		,CASE WHEN NbJoursEcart&gt;0 THEN NbJoursEcart ELSE 0 END NbJoursRetard
		,CASE WHEN NbJoursEcart&gt;0 THEN  'échue' ELSE 'non échue' END Statut
	FROM(
	SELECT	CASE WHEN @rupture=1 THEN DE.DE_No ELSE 0 END DE_No
			,CASE WHEN @rupture=1 THEN DE_Intitule ELSE '' END DE_Intitule
			,L.DO_Piece
			,L.CT_Num
			,C.CT_Intitule
			,ISNULL(ER_NbJour,0) Echeance
			,ISNULL(DATEDIFF(DAY,DR_Date,GETDATE()),0) NbJoursEcart 
			, CAST(L.DO_Date AS DATE)DO_Date
			, CAST(DR_Date AS DATE)DR_Date
			, SUM(ISNULL(DL_MontantTTC,0)) AS DL_MontantTTC 
			, SUM(ISNULL(RC_Montant,0))  AS RC_Montant 
			,SUM(ISNULL(DL_MontantTTC,0)) - SUM(ISNULL(RC_Montant,0)) AS Reste_A_Payer
	FROM _EnteteDoc_ L
	INNER JOIN 	F_COMPTET C 
		ON	L.cbDO_Tiers = C.cbCT_Num 
	LEFT JOIN F_DOCREGL D 
		ON	L.cbDO_Piece=D.cbDO_Piece 
		AND L.DO_Domaine=D.DO_Domaine 
		AND L.DO_Type=D.DO_Type  
	LEFT JOIN (SELECT cbDO_Piece,DO_Domaine,DO_Type, SUM(RC_Montant) RC_Montant 
				FROM F_REGLECH
				GROUP BY cbDO_Piece,DO_Domaine,DO_Type) RE 
	ON	L.cbDO_Piece=RE.cbDO_Piece 
	AND L.DO_Domaine=RE.DO_Domaine 
	AND L.DO_Type=RE.DO_Type
	INNER JOIN  F_DEPOT DE 
		ON L.DE_No = DE.DE_No 
	LEFT JOIN Z_DEPOTSOUCHE DS 
		ON DS.DE_No = DE.DE_No
	LEFT JOIN  F_EMODELER EM 
		ON	EM.MR_No = C.MR_No 
		AND EM.N_Reglement = D.N_Reglement  
	WHERE	((@typeTiers =0 AND L.DO_Domaine = 0) OR (@typeTiers =1 AND L.DO_Domaine = 1))
	AND     ((@facCompta=0 AND (   (@typeTiers =0 AND L.DO_Type in (6,7)) OR (@typeTiers =1 AND L.DO_Type in (16,17)) ) 
				OR (@facCompta=1 AND (@typeTiers =0 AND L.DO_Type in (6)) OR (@typeTiers =1 AND L.DO_Type in (16)) ) 
				OR (@facCompta=2 AND (@typeTiers =0 AND L.DO_Type in (7)) OR (@typeTiers =1 AND L.DO_Type in (17)) ))) 
	AND		('' =@DateDebut OR l.DO_Date &gt;= @DateDebut)
	AND         ('-1'= @type_reg OR ('NR'= @type_reg AND ISNULL(DR_Regle,0) = 0) OR ('R'= @type_reg AND ISNULL(DR_Regle,0) = 1))
	AND         ('' =@DateFin OR l.DO_Date&lt;= @DateFin)
	AND         ('0'=@ClientDebut OR L.CT_NUM&gt;=@ClientDebut) 
	AND ('0'=@ClientFin OR L.CT_NUM&lt;=@ClientFin)
	AND		L.DE_No IN (SELECT DE_No FROM #TMPDEPOT) 
	AND		(@affaire= '0' OR DS.CA_Num = @affaire) 
	GROUP BY CASE WHEN @rupture=1 THEN DE.DE_No ELSE 0 END
			,CASE WHEN @rupture=1 THEN DE_Intitule ELSE '' END
			,L.DO_Piece
			,L.CT_Num
			,CT_Intitule
			,ER_NbJour
			,L.DO_Date
			,DR_Date)A
	 WHERE  ('0'&lt;&gt; @type_reg OR ('NR' = @type_reg AND Reste_A_Payer&lt;&gt;0))
	 AND (@signe=0 OR (@signe=-1 AND DL_MontantTTC&lt;@montant) OR (@signe=1 AND DL_MontantTTC&gt;@montant)) 
AND DL_MontantTTC&lt;&gt;0
ORDER BY NbJoursRetard,DR_Date DESC</CommandText>
      </Query>
